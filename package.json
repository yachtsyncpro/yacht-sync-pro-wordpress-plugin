    "url": "git+https://github.com/YachtsForSaleCollective/yacht-sync-pro-wordpress-plugin"
  },
  "bugs": {
    "url": "https://github.com/YachtsForSaleCollective/yacht-sync-pro-wordpress-plugin/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@wordpress/babel-preset-default": "^7.22.0",
    "@wordpress/scripts": "^30.12.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
